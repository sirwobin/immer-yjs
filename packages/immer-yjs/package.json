{
    "name": "immer-yjs",
    "license": "MIT",
    "description": "Combine immer & y.js",
    "version": "1.1.2",
    "Author": "LCZ",
    "repository": {
        "type": "git",
        "url": "https://github.com/sep2/immer-yjs.git"
    },
    "main": "./dist/immer-yjs.umd.js",
    "module": "./dist/immer-yjs.mjs",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.umd.js"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsc && vite build",
        "test": "NODE_NO_WARNINGS=1 vitest",
        "coverage": "vitest run --coverage",
        "release": "standard-version"
    },
    "peerDependencies": {
        "immer": "^10.1.1",
        "yjs": "^13.6.15"
    },
    "devDependencies": {
        "@types/node": "^20.14.2",
        "immer": "^10.1.1",
        "standard-version": "^9.3.2",
        "typescript": "^5.4.5",
        "vite": "^5.2.12",
        "vite-dts": "^1.0.4",
        "vitest": "^1.6.0",
        "yjs": "^13.6.15"
    },
    "packageManager": "yarn@3.2.0",
    "keywords": [
        "immer",
        "yjs",
        "crdt"
    ]
}
